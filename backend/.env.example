# Backend env example
# Copy to .env and adjust values.

# Database (PostGIS)
DB_NAME=
DB_USER=
DB_PASSWORD=
DB_HOST=
DB_PORT=

# GeoServer
GEOSERVER_BASE_URL=
GEOSERVER_USER=
GEOSERVER_PASSWORD=

# Upload limits (bytes)
# Avoid 413 Request Entity Too Large when uploading GeoPackage/Shapefile.
DJANGO_DATA_UPLOAD_MAX_MEMORY_SIZE=52428800
DJANGO_FILE_UPLOAD_MAX_MEMORY_SIZE=10485760

# Optional: default projected CRS for CSV uploads with X/Y not in degrees
# MCP_CSV_DEFAULT_PROJECTED_CRS=EPSG:10665
